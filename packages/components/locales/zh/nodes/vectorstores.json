                "label": "记录管理器",
                "description": "跟踪记录以防止重复"
            },
            "pineconeIndex": {
                "label": "Pinecone 索引"
            },
            "pineconeNamespace": {
                "label": "Pinecone 命名空间",
                "placeholder": "my-first-namespace"
