    "pinecone": {
        "label": "Pinecone",
        "description": "使用 Pinecone（领先的完全托管托管向量数据库）插入嵌入数据并执行相似性或 MMR 搜索",
        "category": "Vector Stores",
        "credential": {
            "label": "连接凭证"
        },
        "inputs": {
            "document": {
                "label": "文档"
            },
            "embeddings": {
                "label": "嵌入"
            },
